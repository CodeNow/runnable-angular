h2.sub-header
  span.sub-header-text Environment Variables
.small Meh locavore Godard plaid Bushwick photo booth hella, brunch post-ironic umami squid. Normcore health goth heirloom 3 wolf moon, Bushwick jean shorts roof party whatever gluten-free asymmetrical.
  //- ng-repeat = "model in state.dependencies"
ul.list.list-env(
)
  li.list-item.gray
    label.label {{ model.opts.name }}
    //- focus, ng-invalid, ng-valid, ng-pristine
    label.input-group.input-validate(
      ng-class = ""
      ng-repeat = "envModel in model.reqEnv"
    )
      span.input-label {{ state.stack.preEnvMarker }}
      input.input(
        ng-blur = ""
        ng-focus = ""
        placeholder = "envModel.name"
        ng-model = "envModel.name"
      )
      span.input-label {{ state.stack.postEnvMarker }}
footer.modal-footer.clearfix
  button.btn.green(
    ng-show = "state.step === 3"
  ) Build Server

