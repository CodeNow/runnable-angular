header.noscroll.nav-environment-header
  span.grid-block Staging
  branch-commit-popover-button(
    instance = "defaultCluster.master"
  )

ul.nav-container-list
  li.li(
    active-account = "activeAccount"
    instance = "CCC.composeCluster.master"
    instance-navigation
    ng-if = "!CCC.composeCluster.master.attrs.isTesting"
  )

  li.li(
    active-account = "activeAccount"
    instance = "stagingInstance"
    instance-navigation
    ng-repeat = "stagingInstance in CCC.getStagingInstances() track by stagingInstance.attrs._id"
  )
