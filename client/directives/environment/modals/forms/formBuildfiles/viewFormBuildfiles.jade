.clearfix
  .label-col Build Files

//- table of current environment variables
.ace-container.ace-build-files
  .sidebar
    ul.file-tree
      li.folder.in.not-toggleable
        a
          svg.iconnables.filetypes.icons-folder
            use(
              xlink:href = "#icons-folder"
            )
          span.item-name /
        ul
          li.file.not-toggleable
            a
              svg.iconnables.filetypes.icons-repository
                use(
                  xlink:href = "#icons-repository"
                )
              span.item-name api
          li.file
            a
              svg.iconnables.filetypes.icons-file
                use(
                  xlink:href = "#icons-file"
                )
              span.item-name dockerfile
    .sidebar-actions
      button.btn.btn-xs.gray
        svg.iconnables.icons-add
          use(
            xlink:href = "#icons-add"
          )

  pre.pre(
    ui-ace
  )

small.small 😌🐻✌️ #{''}
  a.link(
    href = "//google.com"
    target = "_blank"
  ) More about build files