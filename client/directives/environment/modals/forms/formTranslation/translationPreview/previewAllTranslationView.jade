//- do not show if no preview
  .clearfix(
    ng-if = "!state.recalculating && state.diffs.length"
  )
    label.label.label-jump
      small.small Jump to
      select.select
        optgroup(
          label = "Filename rule"
        )
          option build/index.html
        optgroup(
          label = "String rule"
        )
          option build/index2.html
          option build/assets/img.img
input-col.input-spinner(
  ng-if = "state.recalculating"
)
  small.small Updating
  .spinner-wrapper.spinner-sm.spinner-gray.in(
    ng-include = "'spinner'"
  )

.table.table-diff(
  ng-repeat = "diff in state.diffs"
  diff-display
  file-diff = "diff"
  state = "state"
  actions = "actions"
  preview-all = "true"
)

//- if no preview to show
p.p.empty(
  ng-if = "!state.recalculating && !state.diffs.length"
) There's nothing to see here