.popover.menu.bottom(
  ng-class = "{in: active}"
  ng-style = "popoverStyle.getStyle()"
)
  .arrow.white(
    style = "left: auto; right: 0"
  )
  .popover-content
    ul.popover-list
      li.list-item.popover-list-item
        svg.iconnables
          use(
            xlink:href = "#icons-file-new"
          )
        | File
      li.list-item.popover-list-item
        svg.iconnables
          use(
            xlink:href = "#icons-repository-new"
          )
        | Repository
      li.list-item.popover-list-item
        svg.iconnables
          use(
            xlink:href = "#icons-key-new"
          )
        | SSH Key