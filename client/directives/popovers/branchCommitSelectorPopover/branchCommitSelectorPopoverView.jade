.popover.right.popover-branch-menu(
  ng-class = "{'in': active}"
  ng-style = "popoverStyle.getStyle()"
)
  .arrow.white(
    ng-style = "popoverStyle.getArrowStyle()"
  )

  section.popover-content.list-actions.list-commits(
    branch-commit-list
    instance = "data.instance"
    ng-class = "{'with-menu': $root.featureFlags.multipleWebhooks}"
  )
  section.section-secondary.padding-xs(
    ng-if = "$root.featureFlags.multipleWebhooks"
    ng-include = "'deleteEnvironmentView'"
  )
