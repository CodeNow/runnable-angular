.sidebar-heading.strong(
  ng-class = "{closed: $storage.repoListIsClosed}"
  ng-click = "$storage.repoListIsClosed = !$storage.repoListIsClosed"
)
  svg.iconnables.icons-disclose
    use(
      xlink:href = "#icons-disclose"
    )
  | Repositories
.server-sidebar-item(
  edit-repo-commit
  model = "instance.contextVersion.getMainAppCodeVersion()"
)
//- in-process additional repo
.server-sidebar-item
  .small.strong.text-overflow repo-name
  .btn.gray
    .repo-info.text-overflow.js-repository-branch branch-name
      svg.iconnables.icons-branch
        use(
          xlink:href = "#icons-branch"
        )
    .repo-info.text-overflow.js-repository-commit commit message
      svg.iconnables.icons-commit
        use(
          xlink:href = "#icons-commit"
        )
    .repo-info.small 10 minutes ago
    svg.iconnables.icons-arrow-down
      use(
        xlink:href = "#icons-arrow-down"
      )
