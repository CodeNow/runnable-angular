.popover.menu.bottom.popover-debug(
  ng-class = "{in: active}"
  ng-style = "popoverStyle.getStyle()"
  style = "transform-origin: 90% 0;"
)
  .arrow.white(
    style = "left: auto; right: 1px;"
  )
  .popover-content
    .list.popover-list
      .list-item.popover-list-item.small.disabled Debug fromâ€¦
      a.list-item.popover-list-item.text-overflow.monospace(
        ng-repeat = "command in data"
        ng-if = "$middle && data[$index - 1].imageId"
        ng-click = "actions.launchDebugContainer($event, data[$index - 1])"
      )
        strong(
          ng-bind-html = "command.command"
        )

        svg.iconnables
          use(
            xlink:href = "#icons-link-external"
          )