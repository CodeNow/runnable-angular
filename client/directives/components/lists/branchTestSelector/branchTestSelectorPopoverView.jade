.popover.right.popover-branch-menu(
  ng-class = "{'in': active}"
  ng-style = "popoverStyle.getStyle()"
)
  .arrow.white(
  ng-style = "popoverStyle.getArrowStyle()"
  )

  section.padding-xs.popover-content.list-tests.list-actions.list-commits(
    branch-test-list
    instance = "data.instance"
  )
    .padding-md.grid-block.align-center.justify-center(
      ng-click = "state.loading = false"
      ng-if = "state.loading"
    )
      .spinner-wrapper.spinner-gray.spinner-sm(
        ng-include = "'spinner'"
      )
