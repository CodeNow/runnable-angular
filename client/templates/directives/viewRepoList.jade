ul.repository-group
  li.repository-group-item(
    ng-repeat = "repo in build.contextVersions.models[0].appCodeVersions.models"
  )
    svg.iconnables.icons-git
      use(
        xlink:href = "#icons-git"
      )
    h3.repository-title {{ repo.attrs.repo }}
    button.white.repository-update.tooltip(
      data-title = "Fast-forward {branch} (88 commits behind)"
    ) 88
      svg.iconnables.icons-update
        use(
          xlink:href = "#icons-update"
        )
    .commit(
      toggle-pop-over
      toggle-pop-over-model = "dataInstance.data.popoverCommitSelect.data.show"
    )
      .commit-message bugfix object keypath
      svg.iconnables.icons-commit
        use(
          xlink:href = "#icons-commit"
        )
      span.commit-author cflynn07
      time.commit-time 5 hours ago
    pop-over(
      actions = "dataInstance.data.popoverCommitSelect.actions"
      data = "dataInstance.data.popoverCommitSelect.data"
      template = "viewInstancePopoverCommitSelect"
    )

