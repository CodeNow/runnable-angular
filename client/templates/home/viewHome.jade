header.landing-hero
  //- skrollr requires units on all transitions (even 0)
  .landing-hero-bg.layer-one(
    data-0 = "opacity: 1; transform: translate3d(0,0px,0);"
    data-400 = "opacity: .9; transform: translate3d(0,30px,0);"
  )
  .landing-hero-bg.layer-two(
    data-0 = "opacity: 1; transform: translate3d(0,0px,0);"
    data-400 = "opacity: .75; transform: translate3d(0,60px,0);"
  )
  .landing-hero-bg.layer-three(
    data-0 = "opacity: 1; transform: translate3d(0,0px,0);"
    data-400 = "opacity: .6; transform: translate3d(0,90px,0);"
  )
  nav.landing-nav
      a.logo(
        href = "/"
      )
        svg
          use(
            xlink:href = "#runnable-logo"
          )
  .landing-content
    h1.hero-text I need a box to #{''}
      span.hero-text-swap debug
      | .
    p.hero-text-sub Launch boxes on demand in seconds.
    p.hero-text-sub
      button(
        type = "button"
      ) Learn More
      a.btn
        svg.iconnables.icons-git
          use(
            xlink:href = "#icons-git"
          )
        | Sign Up with Github
