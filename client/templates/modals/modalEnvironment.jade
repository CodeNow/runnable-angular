.modal-backdrop(
  ng-class = "{'in': in}"
)
  .modal-dialog.modal-environment(
    ng-click = "$event.stopPropagation()"
  )
    header.modal-header
      h1.modal-heading Edit Environment
      button.btn.btn-icon.white(
        tooltip = "View List of Boxes"
        tooltip-options = "{\"class\":\"bottom\",\"left\":11,\"top\":30}"
        type = "button"
      )
        svg.iconnables.icons-sidebar
          use(
            xlink:href = "#icons-sidebar"
          )
    main.modal-body.ace-runnable-dark
      pre.pre.ace_editor.ace-tm(
        ng-init = "data.env = [];"
      )
    footer.modal-footer
      button.orange Rebuild
      //- button.green Save
      button.white(
        ng-click = "cancel()"
        type = "button"
      ) Go Back

  .modal-dialog.modal-environment(
    ng-click = "$event.stopPropagation()"
  )
    header.modal-header
      h1.modal-heading Box List
      button.btn.btn-icon.white(
        tooltip = "Close"
        tooltip-options = "{\"class\":\"bottom\",\"left\":11,\"top\":30}"
        type = "button"
      )
        svg.iconnables.icons-close
          use(
            xlink:href = "#icons-close"
          )
    main.modal-body