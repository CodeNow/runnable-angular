//- code_editor
#editor(
  ng-class="{'in': dataBuild.showExplorer, '': !dataBuild.showExplorer}"
)
  button#explorer-toggle.btn-icon(
    ng-click = "dataBuild.showExplorer = !dataBuild.showExplorer"
    ng-init = "dataBuild.showExplorer = true"
    type = "button"
  )
    object.iconnables(
      data = "/build/images/icons/icons-folder-open.svg"
      type = "image/svg+xml"
    )
  //- file_explorer
  aside#explorer
    //- ng-init should remember theme by user
    #controls
      button.toggle-theme.tooltip(
        data-title = "Change Theme"
        ng-class = "{dark: !dataBuild.toggleTheme, light: dataBuild.toggleTheme}"
        ng-click = "dataBuild.toggleTheme = !dataBuild.toggleTheme"
      )
        .theme
    .explorer-header
      h6
        | CONFIG FILES
        button.silver.btn-sm.file-explorer-menu.btn-icon(
          ng-class = "{active: dataBuild.showFileMenu}"
          ng-click = "dataBuild.togglePopover('FileMenu', $event)"
        )
          object.iconnables(
            data = "/build/images/icons/icons-gear.svg"
            type = "image/svg+xml"
          )
          .popover.menu.bottom.fade(
            ng-class = "{in: dataBuild.showFileMenu, 'show-add-repo': dataBuild.showAddRepo, 'show-form': dataBuild.showForm}"
            ng-include = "'viewBuildPopoverFileMenu'"
          )
      ul.tree
        li.folder(
            ng-click = "dataBuild.folderTestOpen = !dataBuild.folderTestOpen"
            ng-class = "{'in': dataBuild.folderTestOpen}"
          )
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-folder.svg"
            type = "image/svg+xml"
          )
          | css
          ul
            li.folder
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-folder.svg"
                type = "image/svg+xml"
              )
              | modules
              ul
                li
                  object.iconnables.filetypes(
                    data = "/build/images/icons/filetypes/icons-file.svg"
                    type = "image/svg+xml"
                  )
                  | components.scss
            li
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-file.svg"
                type = "image/svg+xml"
              )
              | styles.css
            li
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-file.svg"
                type = "image/svg+xml"
              )
              | tables.css
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | favicon.png
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | index.html
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | htc.access
  #editor-toolbar
    #tabs-wrapper
    //- file_tabs
  #select-tabs.dropdown
    button.btn-icon(type = "button")
      object.iconnables(
        data = "/build/images/icons/icons-select.svg"
        type = "image/svg+xml"
      )
    select
      option(selected) Tab 1
      option Tab 2
  #editor-container
    //- code
    pre(ui-ace, ng-click = "dataBuild.isClean = !dataBuild.isClean") Test
