//- code_editor
#editor.in
  //- file_explorer
  aside#explorer
    button#explorer-toggle
      object.iconnables(
        data = "/build/images/icons/icons-folder-open.svg"
        type = "image/svg+xml"
      )
    ul#controls
      li.dark-theme.theme.active.tooltip(data-title = "Dark Theme")
        span
      li.light-theme.theme.tooltip(data-title = "Light Theme")
        span
    .explorer-header
      h6
        | BUILD FILES
        //- file_explorer_menu
        button.silver.btn-sm.btn-popover.file-explorer-menu
          span.glyphicons.cogwheel
          .popover.fade.bottom.menu
            .arrow
              .popover-content
                ul#file-explorer-options.popover-body.col-popover-3
                  li
                    a
                      object.iconnables(
                        data = "/build/images/icons/icons-new-file.svg"
                        type = "image/svg+xml"
                      )
                      | New File
                  li
                    a
                      object.iconnables(
                        data = "/build/images/icons/icons-new-folder.svg"
                        type = "image/svg+xml"
                      )
                      | New Folder
                  li
                    a#add-repo-link
                      object.iconnables(
                        data = "/build/images/icons/icons-octocat.svg"
                        type = "image/svg+xml"
                      )
                      | Add Repo
              #add-repo.popover-body
                form
                  input.form-control(
                    type = "search"
                    placeholder = "Filter Repositories"
                  )
                ul.popover-list
                  li
                    a.select-repo
                      | username/reponame
                      a.btn.btm-sm.silver.glyphicons.cogwheel.select-branch
                    form.add-repo-form
                      input.form-control(placeholder = "Branch or SHA (optional)")
                      a.btn.btn-block.orange Add Repository
      ul.tree
        li.folder(
            ng-click = "dataBuild.folderTestOpen = !dataBuild.folderTestOpen"
            ng-class = "{'in': dataBuild.folderTestOpen}"
          )
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-folder.svg"
            type = "image/svg+xml"
          )
          | css
          ul
            li.folder
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-folder.svg"
                type = "image/svg+xml"
              )
              | modules
              ul
                li
                  object.iconnables.filetypes(
                    data = "/build/images/icons/filetypes/icons-file.svg"
                    type = "image/svg+xml"
                  )
                  | components.scss
            li
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-file.svg"
                type = "image/svg+xml"
              )
              | styles.css
            li
              object.iconnables.filetypes(
                data = "/build/images/icons/filetypes/icons-file.svg"
                type = "image/svg+xml"
              )
              | tables.css
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | favicon.png
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | index.html
        li
          object.iconnables.filetypes(
            data = "/build/images/icons/filetypes/icons-file.svg"
            type = "image/svg+xml"
          )
          | htc.access
  #editor-toolbar
    #tabs-wrapper
    //- file_tabs
    #select-tabs.dropdown
      button(type = "button")
      select
        option(selected) Tab 1
        option Tab 2
  #editor-container
    //- code
    pre(ui-ace, ng-click = "dataBuild.toggleClean()") Test