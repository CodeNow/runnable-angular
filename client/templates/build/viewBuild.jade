nav.sub-header
  h1.build-header.divided-header.animations-off(
    ng-class = "{'in': !dataBuild.isClean }"
  )
    button.btn-icon.silver.tooltip(
      data-title = "View All Builds"
      type = "button"
    )
      object.iconnables.icons-back-table(
        data = "/build/images/icons/icons-back-table.svg"
        type = "image/svg+xml"
      )
    .iconnables.iconnables-container
      object.iconnables.icons-pass(
        data = "/build/images/icons/icons-pass.svg"
        type = "image/svg+xml"
      )
      object.iconnables.icons-asterisk(
        data = "/build/images/icons/icons-asterisk.svg"
        type = "image/svg+xml"
      )
    | Build 1776

  section#main-controls(ng-class = "{'in': !dataBuild.isClean}")
    .clean
      button.orange(type = "button") Run Instance
      .btn-group
        button.silver(type = "button") Rebuild
        button.silver.btn-icon(
          ng-class = "{active: dataBuild.showBuildOptionsClean}"
          ng-click = "dataBuild.togglePopover('BuildOptionsClean', $event)"
          type = "button"
        )
          object.iconnables(
            data = "/build/images/icons/icons-arrow-down.svg"
            type = "image/svg+xml"
          )
          .popover.menu.bottom.fade(
            ng-class = "{in: dataBuild.showBuildOptionsClean}"
            ng-include = "'viewBuildPopoverBuildOptions'"
          )

    .dirty
      .btn-group
        button.green(type = "button") Build
        button.green.btn-icon(
          ng-class = "{active: dataBuild.showBuildOptionsDirty}"
          ng-click = "dataBuild.togglePopover('BuildOptionsDirty', $event)"
          type = "button"
        )
          object.iconnables(
            data = "/build/images/icons/icons-arrow-down-alt.svg"
            type = "image/svg+xml"
          )
          .popover.menu.bottom.fade(
            ng-class = "{in: dataBuild.showBuildOptionsDirty}"
            ng-include = "'viewBuildPopoverBuildOptions'"
          )
      button.silver(type = "button") Discard Changes

