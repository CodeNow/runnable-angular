.welcome
  img.img.runnabear(
    height = "250"
    src = "/build/images/welcome/runnabear.png"
    width = "200"
  )
  form.modal-dialog(
    name = "createTeam"
  )
    animated-panel-container

      animated-panel.panel-name(
        default = "true"
        name = "teamName"
      )
        section.modal-body.fade(
          ng-class = "{'in': isActivePanel()}"
        )
          h3.h3 Create your team
          p.p Team names are used for other members to find and request access to your team.
          .input-wrapper
            input.input.input-validate(
              autocomplete = "off"
              autofocus
              maxlength = "39"
              name = "teamName"
              ng-class = "{'loading': loading}"
              ng-model = "teamName"
              ng-pattern = "/^[a-zA-Z0-9-]*$/"
              placeholder = "e.g. Runnable"
              spellcheck = "false"
              tabindex = "{{isActivePanel() ? '0' : '-1'}}"
            )
            .spinner-wrapper.spinner-sm.in.float-right(
              ng-if = "loading"
              ng-include = "'spinner'"
            )
          //- if name is already taken
          .alert.gray(
            ng-if = "teamName === 'Runnable'"
          ) There’s already a team named {{teamName}}
            button.btn.btn-xs.gray Join Team
          ul.list.list-bulleted.list-validation
            li.list-item(
              ng-class = "{'ng-invalid': createTeam.$error.pattern}"
            ) Use letters, numbers, or hyphens
            li.list-item(
              ng-class = "{'ng-invalid': createTeam.$error.maxlength > 39}"
            ) 39 character maximum
        footer.modal-footer.clearfix.fade(
          ng-class = "{'in': isActivePanel()}"
        )
          //- disable this when name is taken too
          button.btn.btn-md.green.float-right(
            ng-click = "\
              data.currentPanelName = 'githubAuth';\
              goToPanel('githubAuth');\
            "
            ng-disabled = "!teamName"
            tabindex = "{{isActivePanel() ? '0' : '-1'}}"
          )
            //- spinner
            .spinner-wrapper.spinner-sm.spinner-white.in(
              ng-if = "loading"
              ng-include = "'spinner'"
            )
            span Next

