#build-stream-header.sub-header
  h1 Builds for
  .dropdown.recipe
    button.btn.silver(type = "button") @recipe
    select
      option(selected) Master
      option prafulrana
      option runnabro
      option cflynn07
  input.form-control(placeholder = "Search Builds")
  .btn-group
    button.silver(type = "button")
      object(
        data = "/build/images/icons/icons-chevron-left.svg"
        type = "image/svg+xml"
      )
      | Prev 100
    button.silver(type = "button") Next 100
      object(
        data = "/build/images/icons/icons-chevron-right.svg"
        type = "image/svg+xml"
      )

.table-scroll
