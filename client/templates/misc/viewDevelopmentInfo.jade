//- development only
marquee(
  ng-if = "dataApp.data.configEnvironment === 'development'"
  style = "\
     bottom: 0; \
     color: red; \
     font-weight: 500; \
     right: 0; \
     position: absolute; \
     width: 300px; \
     z-index: 9999;"
)
  | API: {{ dataApp.data.configAPIHost }}   #{''}
  | ENV: {{ dataApp.data.configEnvironment }}

