  ol.list.list-bordered.list-divided
    li.list-item(
      ng-class = "{active: data.listExpand}"
      ng-repeat = "instance in instances.models"
    )
      .list-item-row(
        ng-click = "data.listExpand = !data.listExpand"
      )
        h4.h4.header-thin {{ instance.attrs.name }}
        svg.iconnables.icons-arrow-down
          use(
            xlink:href = "#icons-arrow-down"
          )
      .list-item-row
        p.p Fork to a new server to run your code
        button.btn.btn-sm.green(
          ng-click = "fork(instance)"
        ) Fork
      .list-item-row
        p.p Overwrite this server to run your code
        button.btn.btn-sm.green(
          ng-click = "overwrite(instance)"
        ) Overwrite
